                                    <option></option>
                                                            </select>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Alias</small>
                                                            </label>
                                                            <input class="form-control input-sm" id=PurchaseOrder_Alias name=PurchaseOrder.Alias>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Order Reference</small>
                                                            </label>
                                                            <input class="form-control input-sm" id=PurchaseOrder_OrderReference name=PurchaseOrder.OrderReference>
                                                        </div>
                                                    </div>
                                                    <div class=col-md-3>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Valid Before</small>
                                                            </label>
                                                            <input type=date class="form-control input-sm" required data-val=true data-val-required="The ValidityDate field is required." id=PurchaseOrder_ValidityDate name=PurchaseOrder.ValidityDate>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>DateTime Issued</small>
                                                            </label>
                                                            <input type=datetime-local step=1 class="form-control input-sm" data-val=true data-val-required="The DateTimeIssued field is required." id=PurchaseOrder_DateTimeIssued name=PurchaseOrder.DateTimeIssued>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>DateTime Created</small>
                                                            </label>
                                                            <input type=datetime-local step=1 class="form-control input-sm" data-val=true data-val-required="The DateTimeCreated field is required." id=PurchaseOrder_DateTimeCreated name=PurchaseOrder.DateTimeCreated>
                                                        </div>
                                                    </div>
                                                    <div class=col-md-3>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>Terms &Conditions</small>
                                                            </label>
                                                            <textarea class="form-control input-sm" id=PurchaseOrder_TermsAndConditions name=PurchaseOrder.TermsAndConditions></textarea>
                                                        </div>
                                                        <div class=form-group>
                                                            <label>
                                                                <small>
                                                                    Purchase Order No: <span id=PurchaseOrderID class=text-info></span>
                                                                </small>
                                                            </label>
                                                            <br>
                                                            <label>
                                                                <small>
                                                                    Is Committed To Stock: <span id=CommittedToStock class=text-info></span>
                                                                </small>
                                                            </label>
                                                            <br>
                                                            <label>
                                                                <small>
                                                                    Prepared By: <span id=PreparedBy class=text-info></span>
                                                                </small>
                                                            </label>
                                                            <br>
                                                            <label hidden>
                                                                <small>
                                                                    Checked By: <span id=CheckedBy class=text-info></span>
                                                                </small>
                                                            </label>
                                                        </div>
                                                        <div id=updateorder hidden>
                                                            <div class="btn-group pull-right">
